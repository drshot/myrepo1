[DEFAULT]
ignoreip = 127.0.0.1/8
findtime = 600
bantime = 300
maxretry = 5
backend = polling
destemail = root@localhost
banaction = iptables-multiport
mta = sendmail
protocol = tcp
chain = INPUT






[ssh]
enabled = true
port = ssh
filter = sshd
logpath = /var/log/secure
maxretry = 6

[ssh-iptables]
enabled = false